{
    "gitbook": ">=3.2.3",
    "title" : "Loopring API",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "expandable-chapters",
        "custom-favicon",
        "katex",
        "codetabs",
        "search-pro",
        "insert-logo",
        "sharing-plus",
        "flexible-alerts"
    ],
    "pluginsConfig" : {
        "favicon": "./gitbook/images/favicon.ico",
        "insert-logo": {
            "url": "https://loopring.org/images/logo.svg",
            "style": "background: none; max-height: 28px; min-height: 28px"
        },
        "sharing": {
            "weibo": true,
            "facebook": true
        }
    }
}
